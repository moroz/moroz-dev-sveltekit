		"hast-util-to-string": "^2.0.0",
		"highlight.js": "^11.8.0",
		"rehype-stringify": "^9.0.3",
		"remark-html": "^15.0.2",
		"remark-parse": "^10.0.2",
		"remark-rehype": "^10.1.0",
		"remark-smartypants": "^2.0.0",
		"sass": "^1.63.3",
		"unified": "^10.1.2",
		"unist-util-visit": "^4.1.2"
	}
}
